                                 <padding>
                                    <Insets bottom="-3.0" top="-3.0" />
                                 </padding>
                              </TextField>
                                    <ImageView fitHeight="38.0" fitWidth="164.0" pickOnBounds="true" preserveRatio="true" smooth="false" styleClass="pricelabel">
                                       <image>
                                          <Image url="@../img/pricesearch/right_zdz.jpg" />
                                       </image>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                                    </ImageView>
                                    <TextField fx:id="endStation" editable="false" focusTraversable="false" maxHeight="38.0" maxWidth="164.0" minHeight="38.0" minWidth="164.0" prefHeight="38.0" prefWidth="164.0" styleClass="pricelabel" text="土城">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </TextField>
                                    <ImageView fitHeight="38.0" fitWidth="164.0" pickOnBounds="true" preserveRatio="true" styleClass="pricelabel">
                                       <image>
                                          <Image url="@../img/pricesearch/right_cxjg.jpg" />
                                       </image>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                                    </ImageView>
                                    <TextField fx:id="ticketPrice" editable="false" focusTraversable="false" maxHeight="38.0" maxWidth="164.0" minHeight="38.0" minWidth="164.0" prefHeight="38.0" prefWidth="164.0" styleClass="pricelabel" text="9" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets left="10.0" />
                                 </VBox.margin>
                              </TextField>
                           </children>
                        </VBox>

                        <ImageView fitHeight="126.0" fitWidth="126.0" onMouseClicked="#back" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/common/return.jpg" />
                           </image>
                           <VBox.margin>
                              <Insets left="29.0" top="30.0" />
                           </VBox.margin>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </children>
                  </VBox>
               </children>
            </Pane>
         </children>
      </HBox>

    <!-- 弹窗-->
      <HBox fx:id="pricePopUp" layoutX="210.0" layoutY="260.0" spacing="15.0" styleClass="pricePopup" visible="false" managed="false">
         <children>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"  styleClass="pricePopupbtn" text="设为起点">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin></Button>
            <Button alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false"  styleClass="pricePopupbtn" text="设为终点" />
            <Button alignment="CENTER" contentDisplay="CENTER" layoutX="65.0" layoutY="10.0" mnemonicParsing="false"   styleClass="pricePopupbtn" text="取消">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin></Button>
         </children>
         <padding>
            <Insets top="15.0" />
         </padding>
      </HBox>
  <!-- 引入倒计时-->
      <fx:include source="timeCount.fxml" fx:id="timeCount" />
