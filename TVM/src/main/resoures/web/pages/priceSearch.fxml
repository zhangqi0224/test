<AnchorPane prefHeight="652" prefWidth="1024" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.PriceSearchController">
   <children>

      <HBox>
         <children>
            <Pane maxHeight="652.0" maxWidth="840.0" prefHeight="652.0" prefWidth="840.0" styleClass="content" stylesheets="@../css/common.css">
               <children>


                  <Pane fx:id="lineImg">
                     <!--  线路图片-->
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="true" pickOnBounds="true" styleClass="linesearchimg" visible="true">
                        <image>
                           <Image url="@../img/routesearch/line0.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line1.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line2.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line3.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line5.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line6.jpg" />
                        </image></ImageView>
                     <ImageView fitHeight="600.0" fitWidth="840.0" managed="false" pickOnBounds="true" styleClass="linesearchimg" visible="false">
                        <image>
                           <Image url="@../img/routesearch/line9.jpg" />
                        </image>
                     </ImageView>

                  </Pane>


                  <!--  线路按钮-->
                  <HBox fx:id="lineButton" layoutY="601.0" maxHeight="50.0" prefHeight="50.0" prefWidth="840.0">
                     <children>

                        <ImageView fx:id="allLineBtn" fitHeight="50.0" fitWidth="200.0" layoutX="127.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line0.png" />
                           </image>
                        </ImageView>

                        <ImageView fx:id="lineOne" fitHeight="50.0" fitWidth="200.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line1.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="lineTwo" fitHeight="50.0" fitWidth="200.0" layoutX="112.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line2.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="lineThree" fitHeight="50.0" fitWidth="200.0" layoutX="219.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
