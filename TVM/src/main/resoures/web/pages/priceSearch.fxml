                        </ImageView>
                        <ImageView fx:id="lineFive" fitHeight="50.0" fitWidth="200.0" layoutX="326.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line5.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="lineSix" fitHeight="50.0" fitWidth="200.0" layoutX="434.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line6.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </ImageView>
                        <ImageView fx:id="lineNine" fitHeight="50.0" fitWidth="200.0" layoutX="542.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" styleClass="linesearchbutton">
                           <image>
                              <Image url="@../img/routesearch/line9.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>


               </children>
            </Pane>

            <Pane prefHeight="652.0" prefWidth="184.0" styleClass="content_right" stylesheets="@../css/common.css">
               <children>
                  <VBox maxWidth="184.0" prefHeight="652.0" prefWidth="184.0">
                     <children>
                        <Label fx:id="stationName" alignment="CENTER" contentDisplay="CENTER" maxWidth="184.0" prefHeight="35.0" prefWidth="184.0" style="-fx-font-family: Arial;" text="营口道" textFill="#f0f0f0">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin></Label>
                        <Label fx:id="serviceStatus" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" maxWidth="184.0" prefHeight="35.0" prefWidth="184.0" text="正常服务" textFill="#f0f0f0">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin></Label>
                        <Line endX="84.0" startX="-100.0" stroke="ALICEBLUE" strokeWidth="2.0">
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </Line>
                        <VBox prefHeight="300.0" prefWidth="184.0">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <children>

                                    <ImageView fitHeight="38.0" fitWidth="164.0" pickOnBounds="true" preserveRatio="true" styleClass="pricelabel">
                                       <image>
                                          <Image url="@../img/pricesearch/right_qdz.jpg" />
                                       </image>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" top="10.0" />
                                 </VBox.margin>
                                    </ImageView>
                                    <TextField fx:id="startStation" editable="false" focusTraversable="false" maxHeight="38.0" maxWidth="164.0" minHeight="38.0" minWidth="164.0" prefHeight="38.0" prefWidth="164.0" styleClass="pricelabel" text="小白楼">
